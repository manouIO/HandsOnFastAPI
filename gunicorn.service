[Unit]
Description=demo fastapi application
After=network.target

[Service]
User=maryse
Group=maryse
WorkingDirectory=/home/maryse/app/src/HANDSONFASTAPI
Environment="PATH=/home/maryse/app/.venv/bin"
EnvironmentFile=/home/maryse/.env
ExecStart=/home/maryse/app/.venv/bin/gunicorn -w 2 -k uvicorn.workers.UvicornWorker app.main: app --bind 0.0.0.0:8000

[Install]
WantedBy=multi-user.target